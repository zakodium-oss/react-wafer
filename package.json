{
  "name": "react-wafer",
  "version": "1.1.0",
  "description": "Render wafer partition in react",
  "main": "lib/src/index.js",
  "module": "lib-es/src/index.js",
  "types": "lib-es/src/index.d.ts",
  "files": [
    "lib/src",
    "lib-es/src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "clean": "rimraf lib lib-es",
    "npm:publish": "npm t && npm publish --access public && git push --follow-tags",
    "prepublishOnly": "npm run build",
    "watch": "tsc -p tsconfig.es.json -w",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint --ext ts,tsx src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run eslint && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-wafer.git"
  },
  "keywords": [
    "wafer",
    "dicing",
    "physics"
  ],
  "author": "Miguel Asencio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-wafer/issues"
  },
  "homepage": "https://github.com/zakodium/react-wafer#readme",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always",
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/addon-storysource": "^6.4.8",
    "@storybook/react": "^6.4.8",
    "@types/js-quantities": "^1.6.2",
    "@types/react": "^17.0.37",
    "@zakodium/eslint-config": "^5.1.0",
    "eslint": "^8.4.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "cheminfo-types": "^0.9.1",
    "js-quantities": "^1.7.6"
  }
}
